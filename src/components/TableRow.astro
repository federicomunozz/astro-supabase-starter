---
import type { Tables } from "../../supabase/types.ts";

interface Props {
  curso: Tables<"cursos">;
}

const { curso } = Astro.props;
---

<tr class="border-b border-neutral-600">
  <td class="text-xl font-bold">{curso.curso}</td>
  <td><a href={`/cursos/${curso.id}`} class="underline">Ver Curso</a></td>
</tr>
